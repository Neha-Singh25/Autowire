<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="in.sp.beans.Address" id="addr">
    	<property name="houseno" value="101121"></property>
    	<property name="city" value="mzn"></property>
    	<property name="pincode" value="251001"></property>
    </bean>
    <bean class="in.sp.beans.Student" id="stdid">
    	<property name="rollno" value="101"></property>
    	<property name="name" value="neha"></property>
    	<property name="address" ref="addr"></property>
    </bean>

</beans>